<% unless @media.empty? %>
$('.pagination').replaceWith('<%= j will_paginate(@media) %>');
$('#masonry-container').html("<%=j render(:partial => 'search', :locals => {:media => @media})%>");
var current_filters = "<%= @current_filters %>"
pollRedis(current_filters)
$('#masonry-container').masonry('reload');

$('.yt_parent').each(function(){
	var newPlayer = new YouTubePlayer($(this));
	newPlayer.ready();
});